{
  "name": "boom2udmf",
  "version": "1.0.0",
  "description": "Vanilla/Boom to UDMF converter",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve",
    "test": "node test/test.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "no-use-before-define": "off",
      "max-classes-per-file": "off",
      "no-bitwise": "off",
      "consistent-return": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PROPHESSOR/boom2udmf.git"
  },
  "keywords": [
    "doom",
    "UDMF",
    "universal",
    "doom",
    "map",
    "format",
    "boom",
    "vanilla",
    "wad"
  ],
  "author": "PROPHESSOR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PROPHESSOR/boom2udmf/issues"
  },
  "homepage": "https://github.com/PROPHESSOR/boom2udmf#readme"
}
